{
  "name": "@telnyx/rtc-sipjs",
  "version": "1.3.6",
  "description": "Javascript API Library for Telnyx web telephony system.",
  "main": "dist/telnyx-rtc-sipjs.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "docs": "jsdoc2md src/lib/telnyx-device.js > docs/TelnyxDevice.md && jsdoc2md src/lib/telnyx-call.js > docs/TelnyxCall.md",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build:webpack": "webpack",
    "build": "npm run build:webpack",
    "watch": "webpack --watch",
    "start": "npm run watch",
    "test": "karma start",
    "release": "telnyx-npm-release",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run release"
  },
  "contributors": [
    {
      "name": "Jason Brackins",
      "email": "jasonb@telnyx.com",
      "url": "https://telnyx.com"
    },
    {
      "name": "Sua Yoo",
      "email": "sua@telnyx.com",
      "url": "https://telnyx.com"
    }
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-telnyx/telnyx-rtc-sipjs"
  },
  "publishConfig": {
    "@telnyx:registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "es6-event-emitter": "^1.5.2",
    "jasmine-core": "^2.5.2",
    "jsdoc-to-markdown": "^2.0.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantomjs-polyfill": "0.0.2",
    "rimraf": "^2.5.4",
    "webpack": "2.2.0-rc.2"
  },
  "dependencies": {
    "sip.js": "^0.7.7"
  },
  "optionalDependencies": {
    "@telnyx/release": "^0.5.1",
    "@telnyx/rtc-mocks": "^1.0.3"
  },
  "engines": {
    "node": ">=4.0.0"
  }
